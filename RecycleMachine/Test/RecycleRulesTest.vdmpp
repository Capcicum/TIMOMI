class RecycleRulesTest is subclass of TestCase, GLOBAL
	
values
 
Rules = {mk_RecycleItem(<A>, true, <Disp>),
mk_RecycleItem(<B>, true, <Disp>),
mk_RecycleItem(<C>, true, <Disp>),
mk_RecycleItem(<A>, true, <GUHL>),
mk_RecycleItem(<B>, true, <PUL>),
mk_RecycleItem(<C>, true, <GOHL>),
mk_RecycleItem(<C>, true, <POL>),
mk_RecycleItem(nil, true, <GUHL>),
mk_RecycleItem(nil, true, <PUL>),
mk_RecycleItem(nil, true, <GOHL>),
mk_RecycleItem(nil, true, <POL>),
mk_RecycleItem(nil, false, <GUHL>),
mk_RecycleItem(nil, false, <PUL>),
mk_RecycleItem(nil, false, <GOHL>),
mk_RecycleItem(nil, false, <POL>),
mk_RecycleItem(<A>, false, <GUHL>),
mk_RecycleItem(<B>, false, <PUL>),
mk_RecycleItem(<C>, false, <GOHL>),
mk_RecycleItem(<C>, false, <POL>),
mk_RecycleItem(nil, true, <Can>),
mk_RecycleItem(nil, false, <Can>),
mk_RecycleItem(<A>, true, nil),
mk_RecycleItem(<B>, true, nil),
mk_RecycleItem(<C>, true, nil)}

operations 

protected runTest : () ==> ()
runTest() == 
	(IsRecycleValidTest();	
	GetBoxPriceTest());

protected IsRecycleValidTest : () ==> ()
IsRecycleValidTest() ==
	(dcl rr : RecycleRules := new RecycleRules(); 
	for all x in set Rules do assertTrue(rr.IsRecycleValid(x)));
	
protected GetBoxPriceTest : () ==> ()
GetBoxPriceTest() == 
	(dcl rr : RecycleRules := new RecycleRules();
	assertTrue(rr.GetBoxPrice() = 12.5));
	

end RecycleRulesTest