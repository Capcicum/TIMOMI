class tokenTest is subclass of TestCase

values

types

operations 

protected runTest : () ==> ()
runTest() == 
	(tokentest();
	mapTest(););

protected tokentest : () ==> ()
tokentest() ==
	(dcl t1 : token := mk_token(1),
	 t2 : token := mk_token(1),
	 t3 : token := mk_token(2);
	assertTrue(t1 = t1);
	assertTrue(t1 = t2);
	assertTrue(t3 <> t1););
	
protected mapTest : () ==> ()
mapTest() == 
	(dcl mp1 : map nat to nat := {2|->3, 4|->6},
	 mp2 : map nat to nat := {2|->3};
	assertTrue(card dom ({2} <: mp1) = 1);
	assertTrue(card dom ({2} <-: mp2) = 0))
	


end tokenTest
