class Car is subclass of GLOBAL

--make to record in global
	   		         
instance variables
	parkingPermit: bool;
	licensePlate : LicensePlate;

operations 

public Car : LicensePlate * bool  ==> Car
Car(plp, ppp) == (
	licensePlate := plp;
	parkingPermit := ppp);


pure public getLicensePlate : () ==> LicensePlate
getLicensePlate() ==
  return licensePlate;
  
pure public hasParkingPermit : () ==> bool
hasParkingPermit() ==
	return parkingPermit;

end Car

