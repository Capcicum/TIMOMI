class TransactionService is subclass of GLOBAL

values
parkingRateDB : map CountryIdentifier to nat1
						= {<Danish> |-> 5,
						 	<European> |-> 10,
						 	<Other> |-> 15};

	
functions

public calculateParkingFee : LicensePlate * PaymentInfo -> nat
calculateParkingFee(plp, ppi) ==
	if is_ParkingCard(ppi)
		 then 0
	else parkingRateDB(plp.ci); -- * (currentTime - parkingTime);



end TransactionService